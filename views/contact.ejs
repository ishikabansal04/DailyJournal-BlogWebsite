<div class="row">
    <div class="col-12 col-md-5 mt-4">
        <h1 class = "main-heading">Subscribe to Our Blogs!</h1>
        <form action="/contact" method="post">
            <div class = "form-group text-center">
                <!-- <label for="fname">First Name</label> -->
                <input class = "form-control" type="text" id = "fname" name ="fname" placeholder = "First Name" required autofocus>
            </div>
            <div class = "form-group">
                <!-- <label for="lname">Last Name</label> -->
                <input class = "form-control" type="text" id = "lname" name ="lname" placeholder = "Last Name" required>
            </div>
            <div class = "form-group">
                <!-- <label for="email">Email Address</label> -->
                <input class = "form-control" type="text" id = "email" name ="email" placeholder = "Email Address" required>   
            </div>
            <div class = "submit-button">
                <button class = "btn btn-primary text-center" type="submit">Submit   <i class="bi bi-arrow-up-right-circle"></i></button>
            </div>
        </form>
    </div>
    <div class="col-12 col-md-6 offset-md-1 order-first order-md-last contact-img mt-0">
        <img src = "/images/contact.jpg" width = "100%">
    </div>
</div>
<%- include("partials/footer") %> 